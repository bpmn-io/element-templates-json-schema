{
  "name": "element-templates-json-schema",
  "version": "0.0.0",
  "description": "JSON Schema for (Camunda) Element Templates",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "source": "lib/index.js",
  "scripts": {
    "build": "rollup -c",
    "distro": "run-s build test:build",
    "lint": "eslint .",
    "prepublishOnly": "run-s distro",
    "dev": "mocha -r esm --reporter=spec --recursive --watch test/spec",
    "test": "nyc --reporter=lcov mocha -r esm --reporter=spec --recursive test/spec",
    "test:build": "mocha --reporter=spec --recursive test/distro",
    "all": "run-s lint test distro"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/element-templates-json-schema.git"
  },
  "keywords": [
    "json-schema",
    "element-templates"
  ],
  "author": "Niklas Kiefer <niklas.kiefer@camunda.com (https://github.com/pinussilvestrus)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpmn-io/element-templates-json-schema/issues"
  },
  "homepage": "https://github.com/bpmn-io/element-templates-json-schema#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-plugin-bpmn-io": "^0.11.0",
    "esm": "^3.2.25",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rollup": "^2.28.2"
  },
  "dependencies": {
    "ajv": "^6.12.5",
    "min-dash": "^3.5.2"
  }
}
